{ "language": "Haskell2010"
, "extensions":
  [ "DataKinds"
  , "DeriveAnyClass"
  , "DeriveGeneric"
  , "DerivingVia"
  , "EmptyCase"
  , "FlexibleContexts"
  , "FlexibleInstances"
  , "GADTs"
  , "LambdaCase"
  , "MultiParamTypeClasses"
  , "NamedFieldPuns"
  , "NoImplicitPrelude"
  , "GeneralizedNewtypeDeriving"
  , "OverloadedStrings"
  , "ScopedTypeVariables"
  , "StrictData"
  , "TypeApplications"
  , "TypeFamilies"
  , "TypeOperators"
  , "TupleSections"
  ]
, "style": "base"
, "fixities":
  [ "infixl 9 ^."
  , "infix  4 ==."
  , "infix  4 >=."
  , "infix  4 >."
  , "infix  4 <=."
  , "infix  4 <."
  , "infix  4 !=."
  , "infixr 3 &&."
  , "infixl 2 ||."
  , "infixl 6 +."
  , "infixl 6 -."
  , "infixl 7 /."
  , "infixl 7 *."

  ]
, "formatting":
  { "penalty":
    { "indent": 1
    , "linebreak": 100
    , "max-line-length": 80
    , "overfull": 9999
    , "overfull-once": 999999
    }
  , "layout":
    { "app": "try-oneline"
    , "con-decls": "vertical"
    , "declaration": "try-oneline"
    , "export-spec-list": "vertical"
    , "if": "flex"
    , "import-spec-list": "vertical"
    , "infix-app": "try-oneline"
    , "let": "vertical"
    , "list-comp": "flex"
    , "record": "try-oneline"
    , "type": "try-oneline"
    }
  , "indent":
    { "onside": 4
    , "deriving": 4
    , "where": 2
    , "case": "indent-by 4"
    , "class": "indent-by 4"
    , "do": "indent-by 4"
    , "export-spec-list": "indent-by 4"
    , "if": "indent-by 4"
    , "import-spec-list": "indent-by 4"
    , "let": "indent-by 4"
    , "let-binds": "indent-by 4"
    , "let-in": "indent-by 4"
    , "multi-if": "indent-by 4"
    , "typesig": "indent-by 4"
    , "where-binds": "indent-by 2"
    }
  , "align":
    { "limits": [ 10, 100 ]
    , "case": true
    , "class": true
    , "import-module": true
    , "import-spec": true
    , "let-binds": true
    , "matches": true
    , "record-fields": true
    , "where": true
    }
  , "group":
    { "share in expression":
      { "force-linebreak": false
      , "spaces": "after"
      , "linebreaks": "none"
      }
    }
  , "op":
    { "default":
      { "force-linebreak": false
      , "spaces": "both"
      , "linebreaks": "before"
      }
    , "=":
      { "force-linebreak": false
      , "spaces": "both"
      , "linebreaks": "after"
      }
    , ",":
      { "force-linebreak": false
      , "spaces": "after"
      , "linebreaks": "before"
      }
    , "@":
      { "force-linebreak": false
      , "spaces": "none"
      , "linebreaks": "none"
      }
    , ". in expression":
      { "force-linebreak": false
      , "spaces": "both"
      , "linebreaks": "before"
      }
    , ">>= in expression":
      { "force-linebreak": true
      , "spaces": "both"
      , "linebreaks": "before"
      }
    , "$":
      { "force-linebreak": true
      , "spaces": "both"
      , "linebreaks": "before"
      }
    , "record in pattern":
      { "force-linebreak": true
      , "spaces": "none"
      , "linebreaks": "none"
      }
    , "record":
      { "force-linebreak": true
      , "spaces": "after"
      , "linebreaks": "after"
      }
    , "= in declaration":
      { "force-linebreak": false
      , "spaces": "both"
      , "linebreaks": "after"
      }
    , ":: in declaration":
      { "force-linebreak": true
      , "spaces": "both"
      , "linebreaks": "before"
      }
    , ":- in type":
      { "force-linebreak": true
      , "spaces": "after"
      , "linebreaks": "before"
      }
    , ":> in type":
      { "force-linebreak": true
      , "spaces": "after"
      , "linebreaks": "before"
      }
    , ". in type":
      { "force-linebreak": true
      , "spaces": "after"
      , "linebreaks": "before"
      }
    , "-> in declaration":
      { "force-linebreak": false
      , "spaces": "after"
      , "linebreaks": "before"
      }
    }
  , "options":
    { "align-sum-type-decl": true
    , "preserve-vertical-space": false
    , "decl-no-blank-lines": []
    , "sort-import-lists": true
    , "sort-imports":
      [ { "prefixes": [ "Roll.Prelude" ]
        , "order": "grouped"
        }
      , { "prefixes": [ "Roll" ]
        , "order": "grouped"
        }
      , { "prefixes": [ "" ]
        , "order": "sorted"
        }
      ]
    , "sort-pragmas": true
    , "flexible-oneline": false
    , "split-language-pragmas": true
    }
  }
}
